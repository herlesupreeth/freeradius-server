server eap-aka-prime {
	namespace = eap-aka-prime

	recv Identity-Response {
		"%{debug_attr:&session-state:}"
		if (!&session-state:Tmp-String-0) {
			update reply {
				&Any-ID-Req := yes
			}
			update session-state {
				&Tmp-String-0 := yes
			}
		}
		ok
	}

	send Identity-Request {
		ok
	}

	send Challenge-Request {
                update control {
                        &SIM-Ki  := 0x8baf473f2f8fd09487cccbd7097c6862
                        &SIM-Opc := 0x8e27b6af0e692e750f32667a3b14605d
                        &SIM-SQN := 0
                        &SIM-Algo-Version := 5
                }
		update reply {
			&Next-Reauth-Id := ""
			&Next-Pseudonym := ""
		}
	}

	store session {
		eap_aka_prime_cache
	}

	load session {
		eap_aka_prime_cache
	}

	clear session {
		eap_aka_prime_cache
	}
}